  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "rollup": "^1.11.3",
    "uglify-es": "^3.3.9"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
